citHeader("To cite the 'ecdm' R package and methods paper in publications use:")

citEntry(entry = "Manual",
         title        = "{ecdm: Exploratory Cognitative Diagnostic Models}",
         author       = personList(
                                   as.person("James Joseph Balamuta"),
                                   as.person("Steven Andrew Culpepper"),
                                   as.person("Jeffrey A. Douglas")
                                  ),
         year         = 2020,
         textVersion  = paste("Balamuta, J. J., Culpepper, S. A., and Douglas, J. A. (2020).",
                              "ecdm: Exploratory Cognitative Diagnostic Models.",
                              "URL https://cran.r-project.org/package=ecdm.")
)

citEntry(entry   = "Article",
         title   = "{Fitting and Analyzing Exploratory Cognitive Diagnostic Models with the ECDM R Package}",
         author  = personList(
                      as.person("James Joseph Balamuta"),
                      as.person("Steven Andrew Culpepper"),
                      as.person("Jeffrey A. Douglas")
                   ),
         journal = "APM",
         year    = "2020",
         url     = "TBA",
         volume  = "Vol",
         number  = "1",
         pages   = "Start-End",
         doi     = "ID",
         textVersion = paste("Balamuta, J. J., Culpepper, S. A., and Douglas, J. A. (2020).",
                             "Fitting and Analyzing Exploratory Cognitive Diagnostic Models with the ECDM R Package."#,
                             #"APM. Vol, Start-End.",
                             #"DOI: ID."
                             )
)
